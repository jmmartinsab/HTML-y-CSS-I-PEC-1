  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-nightly.636",
    "autoprefixer": "^10.2.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.566",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "hamburgers": "^1.1.3"
  }
